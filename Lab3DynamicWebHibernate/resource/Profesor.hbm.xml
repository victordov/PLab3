<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="md.victordov.lab.vo">
	<class name="Profesor" table="profesor">
		<id name="profesorId" type="int">
			<column name="p_id" />
			<generator class="assigned" />
		</id>
		<property name="nume" type="java.lang.String">
			<column name="nume" />
		</property>
		<property name="prenume" type="java.lang.String">
			<column name="prenume" />
		</property>
		<property name="adresa" type="java.lang.String">
			<column name="adresa" />
		</property>
	</class>
</hibernate-mapping>
