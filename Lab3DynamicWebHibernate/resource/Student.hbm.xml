<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="md.victordov.lab.vo">
    <class name="Student" table="student">
        <id name="studentId" type="int">
            <column name="s_id" />
            <generator class="assigned" />
        </id>
        <property name="nume" type="java.lang.String">
            <column name="nume" />
        </property>
        <property name="prenume" type="java.lang.String">
            <column name="prenume" />
        </property>
        <property name="grupa" type="java.lang.String">
            <column name="grupa" />
        </property>
        <property name="email" type="java.lang.String">
            <column name="email" />
        </property>
        <property name="telFix" type="java.lang.String">
            <column name="telFix" />
        </property>
    </class>
</hibernate-mapping>
