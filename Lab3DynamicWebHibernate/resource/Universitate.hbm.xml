<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="md.victordov.lab.vo">
    <class name="Universitate" table="universitate">
        <id name="universitateId" type="int">
            <column name="u_id" />
            <generator class="assigned" />
        </id>
        <property name="numeUniversitate" type="java.lang.String">
            <column name="nume_univer" />
        </property>
        <property name="adresa" type="java.lang.String">
            <column name="adresa" />
        </property>
        <property name="telefon" type="java.lang.String">
            <column name="telefon" />
        </property>
    </class>
</hibernate-mapping>
